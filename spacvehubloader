-- StarterGui.SpaceHubIntro.handler
local function C_2()
local script = G2L["2"];
	-- variables
	local plr = game:GetService("Players").LocalPlayer
	local ts = game:GetService("TweenService")
	local ti = TweenInfo.new(2, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0)
	local ti2 = TweenInfo.new(1, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0)
	local textHolder = script.Parent.textHolder
	local name = textHolder.TextLabel
	local icon = script.Parent.ImageLabel
	local root = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart")
	local shadow = script.Parent.textHolder.DropShadowHolder.DropShadow
	local cam = game:GetService("Workspace").CurrentCamera
	local blur = Instance.new("BlurEffect", game:GetService("Lighting"))
	local sound = Instance.new("Sound", script.Parent)
	local strVal = Instance.new("StringValue", script.Parent)
	--
	
	-- properties
	sound.SoundId = "rbxassetid://11834806248"
	sound.Looped = false
	blur.Name = "BlurEffect"
	blur.Size = 0
	blur.Enabled = true
	textHolder.Size = UDim2.new(0, 0, 0, 50)
	icon.Position = UDim2.new(0.5, 0, -0.5, 0)
	shadow.ImageTransparency = 1
	strVal.Name = "strVal"
	--
	
	-- animations
	local blurIn = ts:Create(blur, ti, {Size = 11})
	local blurOut = ts:Create(blur, ti, {Size = 0})
	
	local On = ts:Create(textHolder, ti2, {Size = UDim2.new(0, 250, 0, 50)})
	local Off = ts:Create(textHolder, ti2, {Size = UDim2.new(0, 0, 0, 50)})
	
	local OnCam2 = ts:Create(cam, ti, {MaxAxisFieldOfView = 80})
	local OffCam2 = ts:Create(cam, ti, {MaxAxisFieldOfView = 108})
	
	local OnCam = ts:Create(cam, ti, {CFrame = CFrame.new(34, 240, 34) * CFrame.Angles(math.rad(52),0,0)})
	local OffCam = ts:Create(cam, ti, {CFrame = plr.Character:WaitForChild("HumanoidRootPart").CFrame})
	--
	
	-- functions
	function strToClock(str)
		local a = str:split(':')
		local hr, m, s = a[1], a[2], a[3]
		hr, m = hr * 3600, m * 60
		local CT = hr + m + s
		return (CT / 60) / 60
	end
	
	function changeTime(timeVal, Dur)
		local goal = {}
		goal.ClockTime = strToClock(timeVal)
		local tweenInfo = TweenInfo.new(Dur, Enum.EasingStyle.Quint, Enum.EasingDirection.Out)
		local tween = ts:Create(game.Lighting, tweenInfo, goal)
		tween:Play()
	end
	
	function Type(item, text)
		for i = 1, #text, 1 do
			item.Text = string.sub(text, 1, i)
			wait()
		end
	end
	
	function playIntro()
		cam.CameraType = Enum.CameraType.Scriptable
		icon.Visible = true
		textHolder.Visible = true
		
		sound:Play()
		blurIn:Play()
		--OnCam:Play()
		--OnCam2:Play()
		
		--repeat wait() until cam.CFrame == CFrame.new(34, 240, 34) * CFrame.Angles(math.rad(52),0,0)
		changeTime("00:00:00", 3)
	
		repeat wait() until blur.Size == 11
	
		On:Play()
		
		for i = 1, 6 do
			wait()
			--shadow.ImageTransparency = (i/10)
			shadow.ImageTransparency = 1-(i/10)
		end
	
		repeat wait() until textHolder.Size == UDim2.new(0, 250, 0, 50)
		
		--shadow.ImageTransparency = 0.8
		
		Type(name, "Welcome to SpaceHub, "..game:GetService("Players").LocalPlayer.DisplayName..".")
	
		textHolder:TweenPosition(UDim2.new(0.5, 0, 0.6, 0), "InOut", "Quint", 1, true)
		wait(0.5)
	
		icon:TweenPosition(UDim2.new(0.5, 0, 0.42, 0), "InOut", "Quint", 2, true)
		
		wait(2)
		repeat wait() until icon.Position == UDim2.new(0.5, 0, 0.42, 0)
		
		for i = 1, 10 do
			wait()
			shadow:Destroy()
			textHolder.Transparency = (i/10)
			name.TextTransparency = (i/10)
		end
		
		icon:TweenPosition(UDim2.new(0.5, 0, 0.5, 0), "InOut", "Quint", 1, true)
		wait(2)
		
		for i = 1, 10 do
			wait()
			icon.ImageTransparency = (i/10)
		end
		
		repeat wait() until icon.ImageTransparency == 1
		--OffCam:Play()
		--OffCam2:Play()
		blurOut:Play()
		
		repeat wait() until blur.Size == 0
		changeTime(strVal.Value, 3)
		
		game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
	end
	--
	
	wait(2)
	playIntro()
end;
task.spawn(C_2);
-- StarterGui.SpaceHubIntro.LocalScript
local function C_b()
local script = G2L["b"];
	local val = script.Parent:WaitForChild("strVal")
	val.Value = game.Lighting.TimeOfDay
end;
task.spawn(C_b);

return G2L["1"], require;

