
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Space Hub | Home Page", HidePremium = false, IntroEnabled = true, SaveConfig = true, IntroText = "SpaceHub", IntroIcon = "rbxassetid://4483362748", Icon = "rbxassetid://4483362748", ConfigFolder = "Loader"})

--tabs
local Tab = Window:MakeTab({
	Name = "Space Hub",
	Icon = "rbxassetid://4483362748",
	PremiumOnly = false
})
local Tab2 = Window:MakeTab({
	Name = "Premium",
	Icon = "rbxassetid://3610239960",
	PremiumOnly = false
})

--notify
OrionLib:MakeNotification({
	Name = "Space Hub",
	Content = "Space Hub Has Loaded!",
	Image = "rbxassetid://4483362748",
	Time = 3
})

--buttons
Tab:AddParagraph("What it does:","This button will load spacehub and check if the game you are in is supported, if not it will load universal")
Tab:AddButton({
	Name = "🪐 Load Space Hub 🪐",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Lucasfin000/SpaceHub/main/gameslocal"))()
  	end    
})
Tab:AddParagraph("What it does:","This button will load spacehub universal and it will work in every game on roblox, it has tons of useful features")
Tab:AddButton({
	Name = "🚀 Load Space Hub Universal 🚀",
	Callback = function()
		loadstring(game:HttpGet(("https://raw.githubusercontent.com/Lucasfin000/SpaceHub/main/spacehubuniversal.lua")))()
  	end    
})
Tab2:AddParagraph("What it does:","This button will load the spacehub premium universal, with tons of features (100+) such as a universal server crasher, audio spammer and flinging, If you do not have a key please purchase one at https://store.orbituniverse.com/")
Tab2:AddButton({
	Name = "💎 Load Space Hub Premium 💎",
	Callback = function()
		loadstring(game:HttpGet(("https://raw.githubusercontent.com/Lucasfin000/SpaceHub/main/updpremiumkeysys")))()
  	end    
})
Tab2:AddParagraph("What it does:","This button will load spacehub premium games with OP features, we support 200+ games for premium, If you do not have a key already please purchase one at https://store.orbituniverse.com/")
Tab2:AddButton({
	Name = "📌 Load Space Hub Premium Games 📌",
	Callback = function()
		loadstring(game:HttpGet(("https://raw.githubusercontent.com/Lucasfin000/SpaceHub/main/walterwhite")))()
  	end    
})
